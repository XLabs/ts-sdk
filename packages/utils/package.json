{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@xlabs-xyz/utils",
  "version": "1.0.0",
  "author": "xLabs",
  "license": "Apache-2.0",
  "description": "misc utils",
  "keywords": ["encoding", "aliasing"],
  "repository": {
    "type": "git",
    "url": "https://github.com/xlabs/ts-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "files": [
    "dist",
    "!*.test.*",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "pnpm test && pnpm build",
    "build": "tsc",
    "test": "tsx --test tests/**/*.test.ts",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "@xlabs-xyz/const-utils": "workspace:*",
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.3.1",
    "@scure/base": "^1.2.4"
  }
}
