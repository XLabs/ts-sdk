{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@xlabs-xyz/const-utils",
  "version": "5.0.1",
  "author": "xLabs",
  "license": "Apache-2.0",
  "description": "Type-safe utilities for readonly and const data structures, including bidirectional mappings, array transformations, and object manipulation, along with general typing utilities",
  "keywords": [
    "mapping",
    "type-safe",
    "readonly",
    "const",
    "tuple",
    "immutable",
    "bidirectional"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xlabs/ts-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "files": [
    "dist",
    "!*.test.*",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsc",
    "test": "tsx --test tests/**/*.test.ts",
    "clean": "rm -rf ./dist"
  }
}
